{
	"name": "chrome-shortcuts-guard",
	"version": "1.0",
	"description": "Make the bepo-xxerty customised keyboard work on sites handling keyboard events in their own way.",
	"manifest_version": 3,
	"content_scripts": [
		{
			"matches": [
				"https://github.com/*",
				"https://gist.github.com/*",

				"https://stackoverflow.com/*",
				"https://*.stackoverflow.com/*",
				"https://superuser.com/*",
				"https://meta.superuser.com/*",
				"https://serverfault.com/*",
				"https://meta.serverfault.com/*",
				"https://askubuntu.com/*",
				"https://meta.askubuntu.com/*",
				"https://*.stackexchange.com/*",
				"https://stackapps.com/*",
				"https://stackoverflowteams.com/*",

				"https://regex101.com/",
				"https://observablehq.com/*",
				"https://atlassian.net/*",
				"https://myshopify.com/*",

				"https://riot.im/app/*",
				"https://app.element.io/*",
				"https://discord.com/*",
				"https://web.whatsapp.com/"
			],
			"js": ["script.js"]
		}
	]
}
